# Blytz.live Environment Variables Example
# Copy this to .env and customize for your environment

# Domain Configuration
DOMAIN=blytz.app
ACME_EMAIL=admin@blytz.app

# Database Configuration
DB_USER=blytz_user
DB_PASSWORD=secure_blytz_password_2024
DB_NAME=blytz_marketplace

# Redis Configuration
REDIS_PASSWORD=secure_redis_password_2024

# JWT Configuration
JWT_SECRET=super_secret_jwt_key_change_in_production_2024
JWT_EXPIRES_IN=168h

# Frontend Configuration
VITE_API_URL=https://api.blytz.app/api/v1
VITE_APP_NAME=Blytz.app Marketplace
VITE_APP_VERSION=1.0.0

# CORS Configuration
CORS_ORIGINS=https://blytz.app,https://api.blytz.app

# Optional: Gemini AI Key (uncomment and add your key)
GEMINI_API_KEY=your-gemini-api-key-here